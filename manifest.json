{
  "manifest_version": 3,
  "name": "Motivation & Schedule Tracker",
  "version": "1.0",
  "description": "Shows trading & IT motivation quotes and scheduled reminders on new tab.",
  "permissions": ["storage", "activeTab", "https://api.openai.com/*"],
  "chrome_url_overrides": {
    "newtab": "newtab.html"
  },
  "icons": {
    "16": "icons/icon16.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  }
}
